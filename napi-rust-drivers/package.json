  "types": "index.d.ts",
  "napi": {
    "name": "wyldcard_drivers",
    "triples": {
      "defaults": false,
      "additional": [
        "armv7-unknown-linux-gnueabihf"
      ]
    }
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@napi-rs/cli": "^2.14.3",
    "ava": "^4.3.3"
  },
